{
  "name": "youtubeSearch",
  "version": "0.1.0",
  "description": "Practice Youtube Search with React, Redux, Webpack",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "dist": "npm run copy & webpack --env=dist",
    "lint": "eslint ./src",
    "lint:test": "eslint ./tests",
    "posttest": "npm run lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "node server.js --env=dev",
    "test": "mocha --compilers js:babel-core/register,css:tests/helpers/skipStyles.js  tests/**/*.spec.js",
    "test:watch": "npm test -- --watch",
    "test:e2e": "mocha-casperjs ./tests/browser/browser.js",
    "coverage": "babel-node ./node_modules/.bin/isparta cover --include '**/*.js' --include '**/*.jsx' --report html ./node_modules/.bin/_mocha -- --compilers css:tests/helpers/skipStyles.js --reporter dot tests/**/*.spec.js",
    "deploy": "npm run dist && firebase deploy"
  },
  "repository": "https://github.com/andorx/youtube-search-react-redux",
  "keywords": [
    "react",
    "redux",
    "caspter",
    "chai",
    "webpack",
    "youtube",
    "apiv3"
  ],
  "author": "Duc N. PHAM",
  "engine": {
    "node": ">=5"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "bower-webpack-plugin": "^0.1.9",
    "casper-chai": "^0.3.0",
    "casperjs": "^1.1.3",
    "chai": "^3.5.0",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.0",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^5.0.0",
    "firebase-tools": "^3.0.7",
    "glob": "^7.0.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.4",
    "jsdom": "^9.4.1",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3",
    "mocha-casperjs": "^0.5.7",
    "nock": "^8.0.0",
    "open": "0.0.5",
    "phantomjs": "^2.1.7",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.2.9",
    "redux-mock-store": "^1.1.2",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "core-js": "^2.0.0",
    "es6-promise": "^3.2.1",
    "es6-shim": "^0.35.1",
    "isomorphic-fetch": "^2.2.1",
    "normalize.css": "^4.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "spectre.css": "^0.1.25"
  }
}
